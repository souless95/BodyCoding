<?xml version="1.0" encoding="UTF-8"?>

<!-- Mapper파일을 구성하기 위한 Doctype 설정 -->
<!DOCTYPE mapper 
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.bc.bodycoding.chatting.chatService">

	<insert id="insertchat" 
		parameterType="global.dto.ChatRoomDTO">
	 	insert into chatroom (roomidx, send_id, roomname, content)
	 	values (chatroom_seq.nextval, #{send_id}, #{roomName}, #{content})
	</insert>

</mapper>